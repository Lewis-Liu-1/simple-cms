<?php
	class TestSectionNames
	{
		const CPRODUCT='Product Group:';
		const TYPE='Test Type:';
		const CATE='Software:';
		const CLASS='Classification:';
		const STANDARD='Standard:';
		const AIM='Aim / Purpose of Test:';
		const REQUIRED='Set-up Instructions/Equipment Required:';
		const PROC='Test Procedure:';
		const EXPECTED='Exp result:';
		const STEPS='Step ';
		
		function getNames()
		{
			return array(self::CPRODUCT,
			self::TYPE,
			self::CATE,
			self::CLASS,
			self::STANDARD,
			self::AIM,
			self::REQUIRED,
			self::PROC,
			self::EXPECTED,
			self::STEPS
			);	
		}
	}

?>