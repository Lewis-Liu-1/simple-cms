<?php

  class SectionName
  {	
  		const COMMENT	="CMT:";
  		const LINK		="A:";
		cont ASM			="ASM:";
		cont C			="C:";
		cont CPLUS		="C++:";
		cont CSHARP		="C#:";
		cont JAVA		="JAVA:";
		cont N			="N:";
		cont IMG			="IMG:";
		cont CMD			="CMD:";
		cont TEST		="TEST:";
		
		public static function getSections()
		{
			return array(self::COMMENT,self::LINK,self::ASM,
							self::C,self::CPLUS,self::CSHARP,
							self::JAVA,self::N,self::IMG,
							self::CMD,self::TEST);		
		}		
  }
?>