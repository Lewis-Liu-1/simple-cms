<?php

class MyData
{
	private $lines;
	function  __construct()
	{
		$this->lines=array();		
	}
			
	public function getLines()
	{
		return $this->lines;
	}
	public function addLine($l)
	{
		$this->lines[]=$l;
	}

}

class MySection
{
	public $Name;
	public $Data;
	
	function __construct($n)
	{
		$this->Name=$n;
		$this->Data=new MyData();
	}	
	public function addLine($l)
	{
		$this->Data->addLine($l);
	}
	
	public function getData()
	{
		return $this->Data->getLines();	
	}	
}
?>
